        function bindCompanyBank() {
        //alert(stat);
        jQuery.ajax({
            type: 'POST',
            url: '@Url.Action("GetApp", "Company")',
            data: '{ }',
            //data: JSON.stringify({ 'stat': stat }),
            contentType: 'application/json; charset=utf-8',
            dataType: 'json',
            success: function (data) {
                jQuery("#tbl-smtp tbody").empty();
                if (jQuery.fn.DataTable.isDataTable("#tbl-smtp")) {
                    jQuery('#tbl-smtp').DataTable().clear().destroy();
                }
                if (data != null) {
                    var i = 1;
                    jQuery.each(data, function (key, value) {
                        var sta = value.Status == null ? "" : value.Status;
                        var row = '<tr>' +
                            '<td>' + i + '</td>' +
                            '<td>' + value.CompName + '</td>' +//1
                            '<td>' + value.PostBox + '</td>' +//3
                            '<td>' + value.Email + '</td>' +//13
                            '<td>' + value.MobNo + '</td>' +//16
                            '<td class="text-end"><span class="badge badge-light-success">' + sta+'</span></td >'+
                            //'<td>' + sta + '</td>' +//17

                            '</tr>';
                        i++;

                        jQuery("#tbl-smtp tbody").append(row);

                    }

                    );
                }
                applyDatatable('tbl-smtp');
            }
        })
    }
    </script>
